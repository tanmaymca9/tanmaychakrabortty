 <div id="login-box">
  <div class="row mainrowcust">
      <div class="col-sm-6 col-md-6 col-lg-6 col-xs-12 padzerosty">
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <%= devise_error_messages! %>
        <div class="left">
          <h1>Sign up</h1>
          <%= f.text_field :email, autofocus: true,placeholder: "Email" %>
          <!-- <input type="text" name="username" placeholder="Username" /> -->
          <% if @minimum_password_length %>
          
          <% end %><br />
          <%= f.password_field :password, autocomplete: "off",placeholder: "Password" %><em>(<%= @minimum_password_length %> characters minimum)</em>
          <!-- <input type="text" name="email" placeholder="E-mail" /> -->
          <%= f.password_field :password_confirmation, autocomplete: "off",placeholder: "Confirm Password" %>
          <!-- <input type="password" name="password" placeholder="Password" />

          <input type="password" name="password2" placeholder="Retype password" />
 -->          
          <!-- <input type="text" name="Number" placeholder="Phone Number" /> -->
          <%= f.text_field :phone_no, autocomplete: "off",placeholder: "Phone " %>
          <!-- <input type="submit" name="signup_submit" value="Sign me up" /> -->
          <div class="field">
          <%= f.label :Role %><br />
          <% User.roles.keys.each do |role| %>
            <%= f.radio_button :role, role %>
            <%= f.label role.to_sym %>
          <% end %>
          <%= f.submit "Sign up" %>
          <p>Already Registered??.. <%= link_to "Sign Insss",users_login_path %></p> 
          </div>
        </div>
        <%end%>
      </div>
      <div class="col-sm-6 col-md-6 col-lg-6 col-xs-12 padzerosty">
          <div class="right">
          <div class="scoilbtnctr">
          
          <button class="social-signin facebook">Sign Up with facebook</button>
          <button class="social-signin twitter">Sign Up with Twitter</button>
          <button class="social-signin google">Sign Up with Google+</button>
        </div>
