									        	</div>
								        	 	<div class="col-md-3 col-sm-6 col-xs-6">
						                    		<button class="btn btn-primary TxtceNtr">Apply Now </button>
								        	 	</div>
						                    </div>
			                  			</div>
					        		</div>
					        	</div>
					        <%end%>
					    <%else%>
			        		<p>No Result Found</p>
			        	<%end%>

</div>