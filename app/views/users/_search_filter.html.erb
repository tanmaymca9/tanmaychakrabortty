								        		<h4 id="<%= school.id %>"><%= school.name %> </h4>
								        		<%#end%>
								        		</a>
								        		<span> <i class="fa fa-map-marker"></i> <%= school.address%>,<%= school.city%>,<%= school.country%> </span>
								            </div>
						                    <div class="col-md-12 col-sm-12 col-xs-12 Anxtysf">
						                    	<div class="col-md-3 col-sm-6 col-xs-6">
