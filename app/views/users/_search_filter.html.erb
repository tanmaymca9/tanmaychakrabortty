  	<h3>Colleges</h3>
			        	<%if @school1.present? %>
				        	<% @school1.each do |school| %>
					        	<div class="bigbox">
					        		<div class="row">
					        			<div class="col-md-12">
								        	<div class="col-md-2">
								        		<% if school.profile_image.present?%>
									                <%= image_tag school.profile_image ,class: "img-thumbnail img-responsive collglogo"%>
									               	<%else%>
									               		<%= image_tag("img.jpg",class: "img-thumbnail img-responsive collglogo")%>
											    <% end %>
								            </div>
								            <div class="col-md-10">
