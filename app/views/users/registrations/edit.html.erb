<%= render "layouts/header1" %>



<%= stylesheet_link_tag  "bootstrap.min" %>
<%= stylesheet_link_tag  "font-awesome.min" %>
<%= stylesheet_link_tag  "style" %>
<%= stylesheet_link_tag  "studentProfile" %>



<%if resource.role == "teacher" %>
  <%= render "users/registrations/teacher_profile" %>
<%elsif resource.role == "school" %>
  <%= render "users/registrations/school_profile" %>
<%elsif resource.role == "student" %>
  <%= render "users/registrations/student_profile" %>
<%elsif resource.role == "vendor" %>
  <%= render "users/registrations/vendor_profile" %>
<%end%>

<%= render "layouts/footer" %>
<%= javascript_include_tag "jquery.min" %>
<%= javascript_include_tag "bootstrap.min" %>
<%= javascript_include_tag "jquery.stellar" %>
<%= javascript_include_tag "jquery.appear" %>
<%= javascript_include_tag "jquery.nicescroll.min" %>
<%= javascript_include_tag "script" %>